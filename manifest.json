{
  "manifest_version": 2,
  "name": "translate-md-viewer",
  "description": "This extension is translate english website to md",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "./dist/options.html"
  },
  "background": {
    "page": "./dist/index.html"
  },
  "icons": {
    "16": "./icon/translate-16.png",
    "48": "./icon/translate-48.png",
    "128": "./icon/translate-128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "https://*/*"
  ],
  "content_security_policy": "style-src https://cdn.jsdelivr.net https://maxcdn.bootstrapcdn.com 'self';script-src https://cdn.jsdelivr.net 'self'; object-src 'self' ;"
}